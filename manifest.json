{
    "name":"Scam Alert",
    "version": "1.0",
    "manifest_version":2,
    "description":"warns potential victims about scams that use legitimate remote viewing software",
    "content_scripts": [
        {
            "matches":["https://www.teamviewer.com/en-us/*", "https://anydesk.com/*",
            "https://www.connectwise.com/*"],
            "js":["warning.js"],
            "css":["styles.css"]
        }
    ]
}
